{
    "name": "ui-service",
    "version": "1.1.0",
    "description": "UI service for notes-app",
    "main": "index.js",
    "scripts": {
        "build": "cd src && npx next build",
        "debug": "node --inspect server/index.js",
        "debug:jest": "node --inspect-brk node_modules/jest/bin/jest.js --env=jsdom --runInBand --watch",
        "dev": "npx cross-env NODE_ENV=development node server/index.js",
        "flow": "node ./scripts/tasks/flow.js",
        "precommit": "lint-staged",
        "postcommit": "git update-index --again",
        "start": "node server/index.js",
        "test": "npx jest --env=jsdom",
        "test:watch": "npx jest --watch --env=jsdom",
        "test:coverage": "npx jest --coverage --env=jsdom"
    },
    "lint-staged": {
        "src/**/*.{js,jsx}": [
            "prettier --write",
            "eslint --fix",
            "jest --bail --findRelatedTests --env=jsdom",
            "git add"
        ],
        "src/**/*.scss": [
            "stylelint --fix",
            "git add"
        ],
        "server/**/*.{js,mjs}": [
            "prettier --write",
            "eslint --fix",
            "git add"
        ]
    },
