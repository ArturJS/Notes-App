{
    "name": "api-service",
    "version": "1.1.0",
    "description": "API service for notes-app",
    "main": "index.js",
    "scripts": {
        "db:create": "npx sequelize db:create",
        "db:migrate": "npx sequelize db:migrate",
        "debug": "node --inspect server/index.js",
        "flow": "node ./scripts/tasks/flow.js",
        "precommit": "lint-staged",
        "start": "node server/index.js",
        "test": "npx jest --env=jsdom",
        "test:watch": "npx jest --watch --env=jsdom",
        "test:coverage": "npx jest --coverage --env=jsdom"
    },
    "lint-staged": {
        "server/**/*.{js,mjs}": [
            "prettier --write",
            "eslint --fix",
            "jest --bail --findRelatedTests --env=jsdom",
            "git add"
        ]
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "babel-eslint": "8.1.2",
        "babel-jest": "22.4.3",
        "babel-plugin-flow-runtime": "0.17.0",
        "eslint": "4.18.2",
        "eslint-config-airbnb-base": "12.1.0",
        "eslint-config-prettier": "2.9.0",
        "eslint-import-resolver-babel-module": "^4.0.0",
        "eslint-plugin-flowtype": "2.41.0",
        "eslint-plugin-import": "^2.8.0",
        "eslint-plugin-jest": "21.15.0",
        "eslint-plugin-prettier": "2.4.0",
        "flow-bin": "0.63.1",
        "flow-runtime": "0.17.0",
        "identity-obj-proxy": "3.0.0",
        "if-env": "1.0.0",
        "jest": "22.4.3",
        "jest-runtime": "22.4.3",
        "lint-staged": "7.1.2",
        "nodemon": "1.17.2",
        "prettier": "1.9.2"
    },
    "dependencies": {
        "@koa/cors": "2.2.1",
        "async-busboy": "0.6.2",
        "axios": "0.18.0",
        "babel-core": "6.26.3",
        "babel-plugin-lodash": "^3.3.4",
        "babel-plugin-module-resolver": "^3.1.1",
        "babel-plugin-syntax-flow": "^6.18.0",
        "babel-plugin-transform-class-properties": "6.24.1",
        "babel-plugin-transform-decorators-legacy": "^1.3.5",
        "babel-plugin-transform-flow-strip-types": "^6.22.0",
